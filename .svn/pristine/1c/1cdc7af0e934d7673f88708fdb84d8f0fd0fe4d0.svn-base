	<!-- 查询学生的团队成果物 -->
	<select id="getTeamAchivementList" resultMap="MoreResultMap"
		parameterType="HashMap">
		SELECT
		b.TM_MEMBER_NAME,a.*
		FROM
		tp_team_achivement a,
		tp_team_member b
		WHERE
		a.ACH_TEAM_ID = b.TM_TEAM_ID
		AND a.ACH_USER_NO = b.TM_MEMBER_NO
		AND a.ACH_PRO_ID = #{proId}
		AND a.ACH_TEAM_ID = #{teamId}
		AND a.ACH_RESULT_TYPE = ${resultType}
		<if test="selectName != null">
		AND a.ACH_RESULT_NAME LIKE #{selectName}
		</if>


	</select>

