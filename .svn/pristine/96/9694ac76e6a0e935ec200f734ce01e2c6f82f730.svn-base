package com.chinasofti.cetp.admin.model;

import java.util.Date;

public class CoinRecord {
    private Long crecId;

    private Integer crecCoin;

    private Integer crecStuNo;

    private Integer crecUserNo;

    private Date crecRegtime;

    private String crecReason;

    public Long getCrecId() {
        return crecId;
    }

    public void setCrecId(Long crecId) {
        this.crecId = crecId;
    }

    public Integer getCrecCoin() {
        return crecCoin;
    }

    public void setCrecCoin(Integer crecCoin) {
        this.crecCoin = crecCoin;
    }

    public Integer getCrecStuNo() {
        return crecStuNo;
    }

    public void setCrecStuNo(Integer crecStuNo) {
        this.crecStuNo = crecStuNo;
    }

    public Integer getCrecUserNo() {
        return crecUserNo;
    }

    public void setCrecUserNo(Integer crecUserNo) {
        this.crecUserNo = crecUserNo;
    }

    public Date getCrecRegtime() {
        return crecRegtime;
    }

    public void setCrecRegtime(Date crecRegtime) {
        this.crecRegtime = crecRegtime;
    }

    public String getCrecReason() {
        return crecReason;
    }

    public void setCrecReason(String crecReason) {
        this.crecReason = crecReason == null ? null : crecReason.trim();
    }
}