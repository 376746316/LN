	<select id="selectQueryLog" parameterType="HashMap"  resultMap="MoreBaseResultMap">
		 select	 
	            a.SL_LOG_ID as SL_LOG_ID
				,a.SL_LOG_TITLE as SL_LOG_TITLE
				,a.SL_LOG_CONTENT as SL_LOG_CONTENT
				,a.SL_REGTIME as SL_REGTIME
				,b.PRAC_NAME as PRAC_NAME
				,c.STU_NAME as STU_NAME
		  from 	TP_STU_LOG a
			    inner join	TP_PRACTICE b
			    		  on	b.PRAC_ID = a.SL_PRAC_ID
			    inner join	TP_STUDENT c
					      on	c.STU_NO = a.SL_STU_ID
		where	1=1 
	  <if test="selecttitle != null">
      		and	a.SL_LOG_TITLE like #{selecttitle}
      </if>
      <if test="selectname !=null">
      		and	c.STU_NAME like #{selectname}
      </if>
      <if test="selectprac !=null">
