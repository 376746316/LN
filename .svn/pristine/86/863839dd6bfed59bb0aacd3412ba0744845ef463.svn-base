      </if>
      <if test="teacherId !=null">
      		and	b.PRAC_TEACHER like #{teacherId}
      </if>
      <if test="selectdata !=null">
      		and	a.SL_REGTIME like #{selectdata}
      </if>
       <if test="stuNo !=null">
            and a.SL_STU_ID = #{stuNo}
       </if>
       order by a.SL_REGTIME desc
	</select>
	<select id="getStuListWithLastUpdDate" parameterType="java.lang.Integer" resultType="com.chinasofti.cetp.practice.model.StuWithLastUpdDate">
		SELECT
			STU_NO AS stuNo,
			STU_USERID AS stuName,
		
		IF (
			GREATEST(
				IFNULL(MAX(SW_WEEKLY_REGTIME), ''),
				IFNULL(MAX(SL_REGTIME), '')
			) = '',
			NULL,
			GREATEST(
				IFNULL(MAX(SW_WEEKLY_REGTIME), ''),
				IFNULL(MAX(SL_REGTIME), '')
			)
		) AS lastUpdDate
		FROM
			tp_prac_person
		INNER JOIN tp_student ON STU_NO = PP_STU_ID
		LEFT JOIN tp_stu_weekly ON (
			SW_PRACTICE_ID = PP_PRAC_ID
			AND SW_STU_ID = PP_STU_ID
		)
		LEFT JOIN tp_stu_log ON (
			SL_PRAC_ID = PP_PRAC_ID
			AND SL_STU_ID = PP_STU_ID
		)
		WHERE
			PP_PRAC_ID = #{pracId}
		GROUP BY
			PP_STU_ID
		ORDER BY
			lastUpdDate DESC
	</select>
