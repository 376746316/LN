package com.chinasofti.cetp.resource.model;

import java.util.Date;

public class ListVideo {
    private Integer lvId;

    private Integer lvListId;

    private Integer lvVideoId;

    private Integer lvSeq;
    
    private String lvImg;
    
    private Date lvUpdatetime;

    private VideoWithBLOBs video;
    
    public String getLvImg() {
    	return lvImg;
    }

	public void setLvImg(String lvImg) {
    	this.lvImg = lvImg;
    }

	public VideoWithBLOBs getVideo() {
    	return video;
    }

	public void setVideo(VideoWithBLOBs video) {
    	this.video = video;
    }

	public Integer getLvId() {
        return lvId;
    }

    public void setLvId(Integer lvId) {
        this.lvId = lvId;
    }

    public Integer getLvListId() {
        return lvListId;
    }

    public void setLvListId(Integer lvListId) {
        this.lvListId = lvListId;
    }

    public Integer getLvVideoId() {
        return lvVideoId;
    }

    public void setLvVideoId(Integer lvVideoId) {
        this.lvVideoId = lvVideoId;
    }

    public Integer getLvSeq() {
        return lvSeq;
    }

    public void setLvSeq(Integer lvSeq) {
        this.lvSeq = lvSeq;
    }

    public Date getLvUpdatetime() {
        return lvUpdatetime;
    }

    public void setLvUpdatetime(Date lvUpdatetime) {
        this.lvUpdatetime = lvUpdatetime;
    }
}