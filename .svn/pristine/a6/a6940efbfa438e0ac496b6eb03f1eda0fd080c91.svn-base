		delete 
		from 
		     tp_prac_person
		where
		      PP_PRAC_ID =#{pracId}
               and PP_STU_ID = #{stuNo}
		
	</delete>
	<select id="selectQueryNotProMng" parameterType="HashMap"  resultMap="BaseResultMap">
		  select	 a.CLASS_ID  
		 			,a.CLASS_NAME
					,a.CLASS_TECH_DIRECT
					,a.CLASS_MAJOR
					,a.CLASS_LINK_MAN
					,a.CLASS_LINK_TEL
					,b.TECH_TYPE_NAME
					,c.MAJOR_NAME
		  from 		TP_CLASS a
   	  left join	TP_TECH_TYPE b
   	  		 on	b.TECH_TYPE_ID = a.CLASS_TECH_DIRECT
   	  left join	TP_MAJOR c
   	  		 on	c.MAJOR_CODE = a.CLASS_MAJOR
		where	a.CLASS_ID 
		 not in	(select 		class_ID 
					   from		TP_PRACTICE d
			     inner join		TP_PRACTICE_CLASS e
			    		 on		e.PC_PRAC_ID = d.PRAC_ID 
			   	 inner join		TP_CLASS f
			   		  	 on		f.CLASS_ID = e.PC_CLASS_ID 
					  where		<![CDATA[d.PRAC_END_DATE>=now()]]>
						and		<![CDATA[d.PRAC_START_DATE<=now()]]>)
	 	<if test="selectclassname != 'null' and selectclassname != '' "  >
     				    and		a.CLASS_NAME like #{selectclassname}
     	</if>
		
	</select>
	<!-- add by YF 2014-7-9 -->
	<select id="getDefaultClass" resultType="java.lang.Integer" parameterType="java.lang.Integer">
	SELECT
		MAX(CLASS_ID) AS CLASS_ID
	FROM
		tp_practice_class
	INNER JOIN tp_class ON PC_CLASS_ID = CLASS_ID
	INNER JOIN tp_practice ON PC_PRAC_ID = PRAC_ID
	WHERE
		PRAC_ID = #{pracId}
	GROUP BY
		PRAC_ID
	</select>
