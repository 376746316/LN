package com.chinasofti.cetp.admin.model;

import java.util.Date;

/**
 * 积分记录页面数据模型
 * @author heyuqiang
 *
 */
public class PageModelScoreRecord {

	/** 名称 **/
	private String srecReason;
	
	/** 周期次数 **/
	private Integer srTimes;
	
	/** 单次奖励分值 **/
	private Integer srScore;
	
	/** 总计奖励分值 **/
	private Integer sumScore;
	
	/** 最新奖励时间 **/
	private Date lastDate;

	public String getSrecReason() {
		return srecReason;
	}

	public void setSrecReason(String srecReason) {
		this.srecReason = srecReason;
	}

	public Integer getSrTimes() {
		return srTimes;
	}

	public void setSrTimes(Integer srTimes) {
		this.srTimes = srTimes;
	}

	public Integer getSrScore() {
		return srScore;
	}

	public void setSrScore(Integer srScore) {
		this.srScore = srScore;
	}

	public Integer getSumScore() {
		return sumScore;
	}

	public void setSumScore(Integer sumScore) {
		this.sumScore = sumScore;
	}

	public Date getLastDate() {
		return lastDate;
	}

	public void setLastDate(Date lastDate) {
		this.lastDate = lastDate;
	}

}
