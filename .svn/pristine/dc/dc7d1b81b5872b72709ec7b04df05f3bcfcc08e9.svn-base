package com.chinasofti.cetp.study.model;

import java.util.Date;

import org.apache.commons.lang.builder.ReflectionToStringBuilder;
import org.apache.commons.lang.builder.ToStringStyle;

public class SpecCourseAndComment {
	private Integer courseID; // 课程ID
	private String courseName; // 课程名称
	private String teacherName;// 授课教师
	private Integer courseCost;// 课程费用
	private String studyingStuNnum; // 学习人数
	private int commentNum;// 评论数目
	private int hitNum;// 浏览数目
	private String imgURL; // 课程图片的url
	private String typeName; // 课程类型名称
	private Date updateTime; //更新时间
	public Date getUpdateTime() {
		return updateTime;
	}

	public void setUpdateTime(Date updateTime) {
		this.updateTime = updateTime;
	}

	public Integer getCourseID() {
		return courseID;
	}

	public void setCourseID(Integer courseID) {
		this.courseID = courseID;
	}

	public String getCourseName() {
		return courseName;
	}

	public void setCourseName(String courseName) {
		this.courseName = courseName;
	}

	public String getTeacherName() {
		return teacherName;
	}

	public void setTeacherName(String teacherName) {
		this.teacherName = teacherName;
	}

	public Integer getCourseCost() {
		return courseCost;
	}

	public void setCourseCost(Integer courseCost) {
		this.courseCost = courseCost;
	}

	

	public String getTypeName() {
		return typeName;
	}

	public void setTypeName(String typeName) {
		this.typeName = typeName;
	}

	public String getImgURL() {
		return imgURL;
	}

	public void setImgURL(String imgURL) {
		this.imgURL = imgURL;
	}

	public int getHitNum() {
		return hitNum;
	}

	public void setHitNum(int scanNum) {
		this.hitNum = scanNum;
	}

	public String getStudyingStuNnum() {
		return studyingStuNnum;
	}

	public void setStudyingStuNnum(String studyingStuNnum) {
		this.studyingStuNnum = studyingStuNnum;
	}

	public int getCommentNum() {
		return commentNum;
	}

	public void setCommentNum(int commentNum) {
		this.commentNum = commentNum;
	}

	@Override
	public String toString() {
		// TODO Auto-generated method stub
		System.out.println(super.toString());
		return ReflectionToStringBuilder.toString(this,
				ToStringStyle.MULTI_LINE_STYLE);
	}

}
