	<select id="selectChapterCompScore" resultMap="ScoreResultMap"
		parameterType="HashMap">
		select
		e.CHAP_ID,e.COURSE_ID,e.SC_ID,e.SPEC_NAME,
		f.theory_score,f.CHAP_NAME,f.STU_NAME, sum(a.ES_SCORE) prac_score,
		a.ES_STU_ID AS stu_no,
		n.NS_SCORE

		from
		(select
		SUM(b.HS_SCORE) theory_score
		,d.CHAP_ID CHAP_ID
		,d.CHAP_NAME
		CHAP_NAME
		,g.STU_NAME STU_NAME
		from
		tp_student
		g,tp_homework_stu
		b,tp_courseware c,tp_chapter d
		where
		g.STU_NO =
		b.HS_STU_ID
		and
		b.HS_COURSE_ID=c.COURSE_ID
		and c.COURSE_ID =
		d.CHAP_COURSE_ID
		and
		g.STU_NO = #{stuNo}
		and d.CHAP_SPEC_ID = #{specId}
		GROUP BY D.CHAP_ID) f
		,(select
		b.SC_ID SC_ID
		,d.CHAP_ID CHAP_ID
		,b.COURSE_ID COURSE_ID
		,h.SPEC_NAME
		from
		tp_sub_courseware
		b,tp_courseware c,tp_chapter
		d,tp_spec_course h
		where
		b.COURSE_ID
		=c.COURSE_ID
		and c.COURSE_ID =
		d.CHAP_COURSE_ID
		and d.CHAP_SPEC_ID =
		h.SPEC_ID
		and h.spec_id =
		#{specId}) e
		,tp_exercise_stu a
		,
		tp_normal_score n
		where
		f.CHAP_ID
		=
		e.CHAP_ID
		AND n.NS_CHAP_ID = e.CHAP_ID
		and e.SC_ID =
		a.ES_SUBCW_ID
		and a.ES_STU_ID =
		#{stuNo}
		group
		by
		e.CHAP_ID
	</select>
	<select id="selectChapterScoreForShow" resultMap="ScoreResultMap"
		parameterType="HashMap">
		SELECT
		e.*, f.theory_score,
		f.CHAP_NAME,
		f.STU_NAME,
		sum(a.ES_SCORE) prac_score,y.NS_SCORE,
		a.ES_STU_ID AS stu_no
		FROM
		(
		SELECT
		SUM(b.HS_SCORE) theory_score,
		d.CHAP_ID CHAP_ID,
		d.CHAP_NAME
		CHAP_NAME,
		g.STU_NAME STU_NAME
		FROM
		tp_student g,
		tp_homework_stu b,
		tp_courseware c,
		tp_chapter d
		WHERE
		g.STU_NO = b.HS_STU_ID
		AND
		b.HS_COURSE_ID = c.COURSE_ID
		AND c.COURSE_ID =
		d.CHAP_COURSE_ID
		AND
		g.STU_NO = #{stuNo}
		AND d.CHAP_SPEC_ID = #{specId}
		GROUP BY
		D.CHAP_ID
		) f,
		(
		SELECT
		b.SC_ID SC_ID,
		d.CHAP_ID CHAP_ID,
		b.COURSE_ID COURSE_ID,
		h.SPEC_NAME
		FROM
		tp_sub_courseware b,
		tp_courseware c,
		tp_chapter d,
		tp_spec_course h
		WHERE
		b.COURSE_ID = c.COURSE_ID
		AND c.COURSE_ID =
		d.CHAP_COURSE_ID
		AND d.CHAP_SPEC_ID =
		h.SPEC_ID
		AND h.spec_id = #{specId}
		) e,
		tp_normal_score y,
		tp_exercise_stu a
		WHERE
		f.CHAP_ID = e.CHAP_ID
		AND
		e.SC_ID = a.ES_SUBCW_ID
		AND a.ES_STU_ID = #{stuNo}
		AND y.NS_CHAP_ID =
		e.CHAP_ID
		AND y.NS_STU_ID = a.ES_STU_ID
		GROUP BY
		e.CHAP_ID
	</select>
	<select id="selectCompScoreForSpecIdAndStuNo" parameterType="HashMap"
		resultMap="ScoreResultMap">

		SELECT
		SUM(sub.theory_score)
		theory_score,SUM(sub.prac_score)prac_score
		FROM(
		select
		f.theory_score,sum(a.ES_SCORE) prac_score
		from
		(select
		SUM(b.HS_SCORE)
		theory_score
		,d.CHAP_ID CHAP_ID
		,d.CHAP_NAME CHAP_NAME
		,g.STU_NAME
		STU_NAME
		from
		tp_student g,tp_homework_stu b,tp_courseware c,tp_chapter
		d
		where
		g.STU_NO = b.HS_STU_ID
		and b.HS_COURSE_ID=c.COURSE_ID
		and
		c.COURSE_ID = d.CHAP_COURSE_ID
		and g.STU_NO = #{stuNo}
		and
		d.CHAP_SPEC_ID = #{specId}
		GROUP BY D.CHAP_ID) f
		,(select
		b.SC_ID SC_ID
		,d.CHAP_ID CHAP_ID
		,b.COURSE_ID COURSE_ID
		,h.SPEC_NAME
		from
		tp_sub_courseware b,tp_courseware c,tp_chapter d,tp_spec_course h
		where
		b.COURSE_ID =c.COURSE_ID
		and c.COURSE_ID = d.CHAP_COURSE_ID
		and
		d.CHAP_SPEC_ID = h.SPEC_ID
		and h.spec_id = #{specId}) e
		,tp_exercise_stu a

		where
		f.CHAP_ID = e.CHAP_ID
		and e.SC_ID =
		a.ES_SUBCW_ID
		and a.ES_STU_ID = #{stuNo}
		group by e.CHAP_ID) sub
	</select>

