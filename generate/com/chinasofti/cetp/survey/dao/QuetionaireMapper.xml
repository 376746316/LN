	<select id="getQuetionaireBySurveyId" resultMap="ResultMapWithBLOBs"
		parameterType="Integer">
		SELECT
			<include refid="Base_Column_List" />
			,
			<include refid="Blob_Column_List" />
		FROM
			TP_QUETIONAIRE
		INNER JOIN TP_SURVEY_QUE ON TP_QUETIONAIRE.QA_ID = TP_SURVEY_QUE.SQA_QUE_ID
