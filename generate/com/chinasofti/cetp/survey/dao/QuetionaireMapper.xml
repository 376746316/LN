	<resultMap id="ModelStatementMap" type="com.chinasofti.cetp.survey.model.ModelStatement">
		<id column="QA_ID" property="qaId" jdbcType="INTEGER" />
		<result column="QA_TITLE" property="qaTitle" jdbcType="LONGVARCHAR" />
		<result column="AGE_SCORE" property="ageScore" jdbcType="INTEGER" />
		<result column="SQA_SEQ" property="sqaSeq" jdbcType="INTEGER" />
		<result column="SY_SURVEY_ID" property="surveyId" jdbcType="INTEGER" />
		<result column="SY_SURVEY_ID" property="surveyId" jdbcType="INTEGER" />
		<result column="QA_PROP" property="prop" jdbcType="CHAR" />
		<association column="{qaId=QA_ID,surveyId=SY_SURVEY_ID}" property="sqiList" resultMap="ChartModelReaultMap" select="get_ID_SQI_Percent" />
	</resultMap>
	<select id="getModelStatementBySySurveyId" resultMap="ModelStatementMap" parameterType="Integer">
		SELECT
			t1.QA_ID,
			t1.QA_TITLE,
			ROUND(AVG(t4.ID_SCORE), 2) as AGE_SCORE,
			t2.SQA_SEQ,
			t3.SY_SURVEY_ID,
			t1.QA_PROP
		FROM
			TP_QUETIONAIRE t1
		INNER JOIN tp_survey_que t2 ON t2.SQA_QUE_ID = t1.QA_ID
		INNER JOIN TP_SURVEY t3 ON t2.SQA_SURVEY_ID = t3.SY_SURVEY_ID
		LEFT JOIN TP_SURVEY_INST_DETAIL t4 ON (
			t1.QA_ID = t4.ID_SURVEY_QUE_ID
			AND t4.ID_SURVEY_ID = #{sySurveyId}
			)
		WHERE
			t3.SY_SURVEY_ID = #{sySurveyId}
		GROUP BY
			t1.QA_ID
	</select>
	  <resultMap type="com.chinasofti.cetp.survey.model.ChartModel" id="ChartModelReaultMap">
  		<result column="id" property="id" jdbcType="INTEGER"/>
  		<result column="nam" property="name" jdbcType="VARCHAR"/>
  		<result column="val" property="value" jdbcType="VARCHAR"/>
  </resultMap>
  <select id="get_ID_SQI_Percent" resultMap="ChartModelReaultMap" parameterType="map">
	 SELECT
		tp_survey_que_item.SQI_ID AS id,
		tp_survey_que_item.SQI_CONTENT AS nam,
		IFNULL(ROUND(
			IFNULL(
				count(
					tp_survey_inst_detail.ID_DETAIL_ID
				),
				0
			) / IFNULL(
				(
					SELECT
						COUNT(0)
					FROM
						tp_survey_inst
					WHERE
						tp_survey_inst.SI_SURVEY_ID = #{surveyId}
				),
				1
			),
			2
		),0) AS val
	FROM
		tp_survey_que_item
	LEFT JOIN tp_survey_inst_detail ON (
		tp_survey_que_item.SQI_ID = tp_survey_inst_detail.ID_RESULT
		AND tp_survey_inst_detail.ID_SURVEY_ID = #{surveyId}
	)
	WHERE
		tp_survey_que_item.SQI_QA_ID = #{qaId}
	GROUP BY
		tp_survey_que_item.SQI_ID
	ORDER BY
		tp_survey_que_item.SQI_ID 
  </select>
