  <select id="selectClass" parameterType="java.lang.Integer"  resultMap="BaseResultMap">
		  select	 c.CLASS_ID  
		 			,c.CLASS_NAME
					,c.CLASS_TECH_DIRECT
					,c.CLASS_MAJOR
					,c.CLASS_LINK_MAN
					,c.CLASS_LINK_TEL
					,g.ORG_NAME SCHOOL_NAME
					,d.TECH_TYPE_NAME
					,f.MAJOR_NAME
		  from 	TP_PRACTICE a
    inner join	TP_PRACTICE_CLASS b
    		 on	b.PC_PRAC_ID = a.PRAC_ID 
   	inner join	TP_CLASS c
   		  	 on	c.CLASS_ID = b.PC_CLASS_ID
      inner join TP_ORG g
             on c.ORG_ID = g.ORG_ID
   	  left join	TP_TECH_TYPE d
   	  		 on	d.TECH_TYPE_ID = c.CLASS_TECH_DIRECT
   	  left join	TP_MAJOR f
   	  		 on	f.MAJOR_CODE = c.CLASS_MAJOR
		where	a.PRAC_ID = #{pracId,jdbcType=INTEGER}
	</select>
	<!-- edit by YF 2013-11-20　 -->
	<select id="notSelectClass" parameterType="map"  resultMap="BaseResultMap">
		  select	 a.CLASS_ID  
		 			,a.CLASS_NAME
					,a.CLASS_TECH_DIRECT
					,a.CLASS_MAJOR
					,a.CLASS_LINK_MAN
					,a.CLASS_LINK_TEL
					,b.TECH_TYPE_NAME
					,c.MAJOR_NAME
					,g.ORG_NAME SCHOOL_NAME
		  from 		TP_CLASS a
	   inner join TP_ORG g
             			 on a.ORG_ID = g.ORG_ID
      <if test="orgName!=null"  >
      		and g.ORG_NAME like #{orgName}
      </if>
   	  left join	TP_TECH_TYPE b
   	  		 on	b.TECH_TYPE_ID = a.CLASS_TECH_DIRECT
   	  left join	TP_MAJOR c
   	  		 on	c.MAJOR_CODE = a.CLASS_MAJOR
		where	a.CLASS_ID 
		 not in	(select 		class_ID 
					   from		TP_PRACTICE d
			     inner join		TP_PRACTICE_CLASS e
			    		 on		e.PC_PRAC_ID = d.PRAC_ID 
			   	 inner join		TP_CLASS f
			   		  	 on		f.CLASS_ID = e.PC_CLASS_ID 
					  where		 not  d.PRAC_STATUS = '4' )
		<if test="className!=null"  >
	      	and a.CLASS_NAME like #{className}
	    </if>
		<if test="orgId!=null"  >
	      	and a.org_id in (
			<foreach collection="orgList" separator="," item="org">
			#{org.orgId}
			</foreach>
			)
	    </if>
	</select>
