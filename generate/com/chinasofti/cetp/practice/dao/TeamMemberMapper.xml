  <!-- 查询实训ID,项目ID -->
	<select id="getTeamMemberMsg" parameterType="java.lang.Integer" resultMap="ResultMapWithId">
		  select		 b.TEAM_PRO_ID
		  				,b.TEAM_PRAC_ID
		    from		TP_TEAM_MEMBER  a
      inner join		TP_PRACTICE_TEAM  b
        	   on		b.TEAM_ID = a.TM_TEAM_ID
          where		a.TM_MEMBER_NO = #{stuNum}  		 
	</select>
	<!-- add by YF 2013-12-2 -->
	<resultMap id="MoreStudentMap" type="com.chinasofti.cetp.admin.model.MoreStudent">
		<id column="STU_NO" property="stuNo" jdbcType="INTEGER" />
		<result column="STU_NAME" property="stuName" jdbcType="VARCHAR" />
		<result column="STU_SEX" property="stuSex" jdbcType="CHAR" />
		<result column="STU_IS_LEADER" property="stuIsLeader" jdbcType="CHAR" />
		<result column="CLASS_NAME" property="className" jdbcType="VARCHAR" />
	</resultMap>
	<select id="getUnselectedTeamMember" resultMap="MoreStudentMap" parameterType="map"> 
		SELECT
			STU_NO,
			STU_NAME,
			STU_SEX,
			STU_IS_LEADER,
			CLASS_NAME
		FROM
			tp_student
		INNER JOIN tp_class ON STU_CLASS_ID = CLASS_ID
		INNER JOIN tp_practice_class ON (
			STU_CLASS_ID = PC_CLASS_ID
			AND PC_PRAC_ID = #{pracId,jdbcType=INTEGER}
		)
		WHERE
			(
				STU_NO NOT IN (
					SELECT
						TM_MEMBER_NO
					FROM
						tp_team_member
					INNER JOIN tp_practice_team ON TM_TEAM_ID = TEAM_ID
					WHERE
						TEAM_PRAC_ID = #{pracId,jdbcType=INTEGER}
				)
		<if test="teamId != null">
				OR STU_NO IN (
					SELECT
						TM_MEMBER_NO
					FROM
						tp_team_member
					WHERE
						TM_TEAM_ID = #{teamId,jdbcType=INTEGER}
				)
		</if>
			)
		<if test="stuName != null">
			AND STU_NAME LIKE #{stuName,jdbcType=VARCHAR}
		</if>
		<if test="className != null">
			AND CLASS_NAME LIKE #{className,jdbcType=VARCHAR}
		</if>
	</select>
</mapper>